{
  "name": "vue-picker-mobile",
  "version": "0.1.99",
  "description": "一款简单的、支持单列多列的、联动非联动的列表选择器",
  "main": "lib/vue-picker-mobile.umd.min.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lib": "vue-cli-service build --target lib --dest lib --name vue-picker-mobile package/index.js"
  },
  "devDependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xml00007/vue-picker-mobile.git"
  },
  "keywords": [
    "vue2.x",
    "vue-picker-mobile"
  ],
  "author": "信明良",
  "license": "ISC",
  "files": [
    "lib/*"
  ],
  "bugs": {
    "url": "https://github.com/xml00007/vue-picker-mobile/issues"
  },
  "homepage": "https://github.com/xml00007/vue-picker-mobile/blob/master/publish/README.md"
}
